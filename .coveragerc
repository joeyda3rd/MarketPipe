[run]
branch = True
parallel = True
data_file = tmp/coverage/.coverage
source = src/marketpipe
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    examples/*

[xml]
output = tmp/coverage/coverage.xml

[html]
directory = tmp/coverage/html

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    \.\.\.
precision = 2
show_missing = true
skip_covered = false 